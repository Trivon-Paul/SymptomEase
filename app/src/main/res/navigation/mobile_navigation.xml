<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_dashboard">


    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.symptomease.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_diagnostics"
        android:name="com.example.symptomease.ui.diagnostics.DiagnosticsFragment"
        android:label="Diagnostics"
        tools:layout="@layout/fragment_diagnostics" />

    <fragment
        android:id="@+id/navigation_saved_symptoms"
        android:name="com.example.symptomease.ui.savedSymptoms.SavedSymptomsFragment"
